
public class Main {

	public static void main(String[] args) {
		// TODO Auto-generated method stub


		Product product1 = new Product();
		product1.setName("Delongi");
		product1.setUnitPrice(200);
		product1.setUnitsInStock(4);
		product1.setImageurl("x1.url");
		
		Product product2 = new Product();
		product2.setName("Delongi2");
		product2.setUnitPrice(200);
		product2.setUnitsInStock(4);
		product2.setImageurl("x1.url");
		

		Product product3 = new Product();
		product3.setName("Delongi2");
		product3.setUnitPrice(200);
		product3.setUnitsInStock(4);
		product3.setImageurl("x1.url");
		
		
		Product[] products = {product1, product2, product3};
		
		for(Product product:products) {
			System.out.println(product.getName());
		}
		
		System.out.println();

		
		
		
		

		
		
	}

